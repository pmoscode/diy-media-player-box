{
    "testEnvironment": "node",
    "modulePaths": [
        "<rootDir>",
        "/node_modules/"
    ],
    "moduleFileExtensions": [
        "js"
    ],
    "collectCoverageFrom": ["app/**/*"],
    "testPathIgnorePatterns": ["/node_modules/"],
    "transformIgnorePatterns": ["/node_modules/"],
    "coverageThreshold": {
        "global": {
            "branches": 80,
            "functions": 75,
            "lines": 80,
            "statements": 80
        }
    },
    "collectCoverage": true
}
